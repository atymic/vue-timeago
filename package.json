{
  "name": "vue-timeago",
  "version": "5.0.0",
  "productName": "Vue Timeago",
  "description": "A timeago filter for Vue.",
  "license": "MIT",
  "repository": "egoist/vue-timeago",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "https://github.com/egoist"
  },
  "scripts": {
    "test": "xo && npm run build",
    "build": "bili",
    "prepublishOnly": "npm run build",
    "example": "poi --serve --config example/poi.config.js",
    "build:example": "poi --prod --config example/poi.config.js"
  },
  "main": "dist/vue-timeago.cjs.js",
  "module": "dist/vue-timeago.es.js",
  "unpkg": "dist/vue-timeago.js",
  "jsdelivr": "dist/vue-timeago.js",
  "cdn": "dist/vue-timeago.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "timeago",
    "vue",
    "filter"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "bili": "^4.7.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "marked": "^0.6.0",
    "poi": "^12.5.6",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "prismjs": "^1.6.0",
    "semantic-release": "^15.13.3",
    "typeface-nunito": "^0.0.72",
    "vue": "^2.6.9",
    "vue-github-badge": "^1.0.1",
    "vue-template-compiler": "^2.6.9",
    "xo": "^0.24.0",
    "date-fns": "^1.29.0",
    "flexboxgrid": "^6.3.1"
  },
  "xo": {
    "extends": [
      "rem",
      "plugin:prettier/recommended"
    ],
    "ignores": [
      "example/**"
    ]
  },
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/*.js": [
      "xo --fix",
      "git add"
    ]
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
